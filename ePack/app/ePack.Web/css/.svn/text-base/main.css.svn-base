body
{
	font-family: "Verdana", arial, helvetica, sans-serif;
	background-color:#fff;
	text-align: center;
}

.global
{
	margin: 0 auto;
	width:1019px;
}

.header
{
	height:85px;
	background: url(/img/titleBar.png) no-repeat left;
	margin-left: 24px;
}
	
/*------Tabs--------*/
.TabsHeader
{
	height: 138px;
	}
.Tabs
{
	background:url(/img/backgMenu.png) no-repeat center;
	height: 40px;
	margin-right: 3px;
	padding-left: 22px;
	}
	
.commonTabs
{
float: left;
width: 140px;
margin-right:2px;
}

.tabArrow
{
	display:none;
	}

.Tabs a
{
	background:url(/img/tabBackground.png) repeat-x;
	padding:8px 10px 8px;
	color:#ffffff;
	font-family:Arial;
	font-size:13px;
	font-weight:bold;
	text-decoration: none;
	float:left;
	width:120px;
	margin-top: 3px;
	margin-bottom: -5px;
}

.tabhovered
{
	background:url(/img/tabHover.png) repeat-x !important;	
	}
	

.Tabs a:hover
{
background:url(/img/tabHover.png) repeat-x;	

	}	
.Tabs .firstTab
{
	height: 40px;
	background:url(/img/tabLeftCorner.png) no-repeat;
	float:left;
	margin-right: 2px;
	width: 140px;
	}


.Tabs .firstTab a
{	
	margin-left:8px;
	width:112px;
	}

.Tabs .lastTab
{
	height: 40px;
	background:url(/img/tabRightCorner.png) no-repeat;
	width:140px;
	background-position:100% 0%;
	float:left;
	margin-top: 2px;
	
}
.Tabs .lastTab a
{
	border-top:solid 0px #350063;
	padding: 8px 2px 8px 10px;
	margin-top:1px;
}

/*----Content-----*/
.content
{
	background:url(/img/fullBg.jpg) repeat-y;
	padding: 5px 35px 15px 25px;
	margin-left:5px;
}
	


